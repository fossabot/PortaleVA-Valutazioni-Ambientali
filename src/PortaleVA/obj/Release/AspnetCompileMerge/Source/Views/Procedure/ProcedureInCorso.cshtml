<div class="boxMappe">

    <div class="boxServiziWebGisVIA">
        <table class="tabellaAdattata">
            <tr class="success">
                <td>
                    <a href="@VariabileService.GetValore("ProcedureVasInCorsoUrl")"
                       class="icona-localizzatoreViaVasCorso icona-testo external servizioWebGis" title="@DizionarioService.TOOLTIP_TooltipLocalizzazione">
                        @string.Format(DizionarioService.PROCEDURE_TestoLinkProgettoCartografico, DizionarioService.PROCEDURE_VasInCorsoLabel)
                    </a>
                </td>
            </tr>
            <tr>
                <td class="alignCenter">
                    <a title="@DizionarioService.TOOLTIP_TooltipLocalizzazione" href="@VariabileService.GetValore("ProcedureVasInCorsoUrl")" class="external">
                        <img class="immagineInterna" src="~/content/images/immagine_vasInCorso.jpg" alt="@string.Format(DizionarioService.PROCEDURE_TestoLinkProgettoCartografico, Model.VoceMenu.GetNome())" />
                    </a>
                </td>
            </tr>
        </table>
    </div>
    <!--div class="table-responsive"-->
