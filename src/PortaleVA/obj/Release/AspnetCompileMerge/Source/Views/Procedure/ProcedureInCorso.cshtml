@model VAPortale.Models.ProcedureViaVasInCorsoModel
@{
    ViewBag.Title = Model.VoceMenu.GetNome();
    Layout = "~/Views/Shared/_LayoutInterno.cshtml";
}
<div class="line_small_title">
    <h2>@Model.VoceMenu.GetNome()</h2>
</div>
<div class="clear"></div>
@if (CultureHelper.GetCurrentCultureShortName().Equals("it", StringComparison.InvariantCultureIgnoreCase))
{
    <p>La reportistica si riferisce alle <strong class="evidenza">procedure in corso</strong> aggiornate in tempo reale.</p>
}
else
{
    <p>Reports refer to <strong class="evidenza">on-going procedures</strong> updated in real time.</p>
}

<br /><br />
<div class="table-responsive">
    <table class="table Procedure">
        <thead>
            <tr>
                <th>@Model.TotaleVas</th>
                <th>@DizionarioService.PROCEDURE_VasInCorso</th>
            </tr>
        </thead>
        @foreach (ConteggioProcedura cp in Model.RigheVas)
        {
            String url = Url.Action("VasElenco", new { id = cp.Procedura.ID, parametro = cp.Parametro });
            <tr>
                <td>@cp.Conteggio</td>
                <td><a title="@cp.Procedura.GetNome()" href="@(cp.Conteggio != 0 ? url : "javascript:void(0)")">@cp.Procedura.GetNome()</a></td>
            </tr>
        }
    </table>
</div>




<br /><br />


